{
  "name": "lmstudio-lan-api-typescript",
  "version": "1.0.0",
  "description": "LM Studio LAN Gateway - TypeScript/Node.js Edition",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npx tsx watch src/index.ts",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "clean": "rmdir /s /q dist 2>nul || echo Clean completed",
    "lint": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "format": "npx prettier --write \"src/**/*.ts\"",
    "type-check": "npx tsc --noEmit",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "check": "npm run lint && npm run type-check && npm run test",
    "service:install": "node scripts/install-service.js",
    "service:uninstall": "node scripts/uninstall-service.js"
  },
  "repository": {
    "type": "git",
    "url": "http://local_proxy@127.0.0.1:60790/git/vuongdat8007/lmstudio-lan-api-typescript"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@lmstudio/sdk": "^1.5.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ipaddr.js": "^2.2.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "node-windows": "^1.0.0-beta.8",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
